  "segments": [
    {
      "text": "Come on, you know, Thanksgiving. Ooh, you got the bigger half. What'd you wish for?",
      "speaker": "Phoebe",
      "predicted_emotion": "happy",
      "sentiment": "positive",
      "utterance_id": 0,
      "season": 1,
      "episode": 9,
