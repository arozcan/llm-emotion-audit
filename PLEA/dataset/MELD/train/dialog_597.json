  "segments": [
    {
      "text": "Hey sweetie!",
      "speaker": "Monica",
      "predicted_emotion": "happy",
      "sentiment": "positive",
      "utterance_id": 0,
      "season": 6,
      "episode": 14,
      "start_time": "0:12:49,824",
      "end_time": "0:12:50,706"
    },
    {
      "text": "Hey!",
      "speaker": "Chandler",
      "predicted_emotion": "happy",
      "sentiment": "positive",
      "utterance_id": 1,
      "season": 6,
      "episode": 14,
      "start_time": "0:12:50,706",
      "end_time": "0:12:52,065"
    },
    {
      "text": "There's no back to this couch!",
      "speaker": "Chandler",
      "predicted_emotion": "surprised",
      "sentiment": "negative",
      "utterance_id": 2,
      "season": 6,
      "episode": 14,
      "start_time": "00:12:59,445",
      "end_time": "00:13:01,696"
    },
    {
      "text": "Why are you reading this? You hate this kind of stuff.",
      "speaker": "Monica",
      "predicted_emotion": "surprised",
      "sentiment": "positive",
      "utterance_id": 3,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:03,532",
      "end_time": "00:13:06,034"
    },
    {
      "text": "Yeah I know, but I figured a shot y'know?",
      "speaker": "Chandler",
      "predicted_emotion": "neutral",
      "sentiment": "neutral",
      "utterance_id": 4,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:06,202",
      "end_time": "00:13:07,786"
    },
    {
      "text": "Maybe one of those stories would make me cry and then you wouldn't think I was y'know, all dead inside.",
      "speaker": "Chandler",
      "predicted_emotion": "neutral",
      "sentiment": "neutral",
      "utterance_id": 5,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:07,953",
      "end_time": "00:13:12,582"
    },
    {
      "text": "Oh that's so sweet! Look Chandler I don't care if you can't cry, I love you.",
      "speaker": "Monica",
      "predicted_emotion": "happy",
      "sentiment": "positive",
      "utterance_id": 6,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:13,209",
      "end_time": "00:13:18,046"
    },
    {
      "text": "Oh that makes me feel so warm in my hollow tin chest.",
      "speaker": "Chandler",
      "predicted_emotion": "neutral",
      "sentiment": "neutral",
      "utterance_id": 7,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:18,214",
      "end_time": "00:13:22,300"
    },
    {
      "text": "Stop it!",
      "speaker": "Monica",
      "predicted_emotion": "happy",
      "sentiment": "positive",
      "utterance_id": 8,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:23,010",
      "end_time": "00:13:25,929"
    },
    {
      "text": "No, I mean, come on, seriously think about it, we get married, we're up at the altar and I'm like this.",
      "speaker": "Chandler",
      "predicted_emotion": "neutral",
      "sentiment": "neutral",
      "utterance_id": 9,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:23,010",
      "end_time": "00:13:29,516"
    },
    {
      "text": "I won't care, because I know you will be feeling it all in here.",
      "speaker": "Monica",
      "predicted_emotion": "neutral",
      "sentiment": "neutral",
      "utterance_id": 10,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:31,352",
      "end_time": "00:13:35,980"
    },
    {
      "text": "Yeah?",
      "speaker": "Chandler",
      "predicted_emotion": "neutral",
      "sentiment": "neutral",
      "utterance_id": 11,
      "season": 6,
      "episode": 14,
      "start_time": "00:13:36,372",
      "end_time": "00:13:38,149"
    }
  ]
