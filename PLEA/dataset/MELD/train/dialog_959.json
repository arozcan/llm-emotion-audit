  "segments": [
    {
      "text": "I didn't even realise how late it was, until I noticed the 5 o'clock shadow on her head.",
      "speaker": "Ross",
      "predicted_emotion": "neutral",
      "sentiment": "neutral",
      "utterance_id": 0,
