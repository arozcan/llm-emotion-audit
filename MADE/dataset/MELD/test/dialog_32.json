      "speaker": "Phoebe Sr",
      "predicted_emotion": "sad",
      "sentiment": "negative",
      "utterance_id": 10,
      "season": 3,
      "episode": 25,
      "start_time": "00:06:39,107",
      "end_time": "00:06:40,857"
