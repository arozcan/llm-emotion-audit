  "segments": [
    {
      "text": "Hello , this is Lucy . May I speak to Mr . Smith ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "Oh , hello , Lucy . What's up ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
      "text": "I'm afraid I can't come to work today , Mr . Smith .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Oh , what's wrong with you ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "I've got a fever and a very sore throat .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Well , you sound ill over the phone .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 3
    },
    {
      "text": "Yes . I have to stay in bed today , but I'll be able to come tomorrow .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 4
    },
    {
      "text": "That's all right . Have a good rest until you feel well enough .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Thank you , Mr . Smith . Goodbye .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "Goodbye , Lucy .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    }
  ]
