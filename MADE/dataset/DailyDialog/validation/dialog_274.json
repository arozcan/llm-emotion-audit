  "segments": [
    {
      "text": "Dear , can you drive me to the clinic ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "I'm afraid I can't .",
