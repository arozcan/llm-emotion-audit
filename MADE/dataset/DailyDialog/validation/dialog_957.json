      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "Make a left on this next street .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Tell me how far to go .",
