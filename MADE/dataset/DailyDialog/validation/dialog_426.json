  "segments": [
    {
      "text": "Excuse me .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Hello sir , may I help you ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "Yes . Can I see that T-shirt on the top shelf please ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "Sure . Here it is .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 4
    },
    {
      "text": "How much does it cost ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "50 dollars .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "50 dollars . That's too much .",
      "speaker": "S1",
      "predicted_emotion": "surprised",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "How about this one ? It's on sale for only 35 dollars .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 3
    },
    {
      "text": "I don't like that one .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "How about the one next to the black gloves ? It's very similar to the one you like .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 3
    },
    {
      "text": "That's nice . How much is it ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "30 dollars .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    },
    {
      "text": "That'll be fine .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 4
    },
    {
      "text": "Is this color OK , or would you like a different color ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 13,
      "dialog_act": 2
    },
    {
      "text": "That blue one's fine .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 14,
      "dialog_act": 1
    },
    {
      "text": "Do you need any more of these shirts ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 15,
      "dialog_act": 2
    },
    {
      "text": "Yes .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 16,
      "dialog_act": 1
    },
    {
      "text": "How many do you want ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 17,
      "dialog_act": 2
    },
    {
      "text": "I'll take two more , a red one and a white one .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 18,
      "dialog_act": 1
    }
  ]
