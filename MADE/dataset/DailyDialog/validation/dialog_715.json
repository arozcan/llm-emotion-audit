  "segments": [
    {
      "text": "Excuse me , is this the parcel counter ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Yes , what can I do for you ?",
