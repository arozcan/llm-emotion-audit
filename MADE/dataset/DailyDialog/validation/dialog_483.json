  "segments": [
    {
      "text": "I need my minibar restocked .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "Everything is gone , sir ?",
