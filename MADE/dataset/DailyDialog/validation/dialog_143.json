  "segments": [
    {
      "text": "Have you bought a bus pass yet ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I'm not getting one .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Why is that ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "It's cheaper if I don't buy one .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Buying a bus pass will save you money .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "How do you figure that ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "There's no limit to how often you can use your bus pass .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Really ?",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "Plus , you don't have to use change for the bus anymore .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "I like that .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "You want to buy one now ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "I'm going to .",
