{
  "id": "dialog_838",
  "segments": [
    {
      "text": "What's wrong , officer ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "You do realize that you ran a red light , don't you ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "I did ?",
      "speaker": "S1",
      "predicted_emotion": "surprised",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "You didn't see the red light ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "I'm sorry for running it , but I really didn't know .",
      "speaker": "S1",
      "predicted_emotion": "sad",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Didn't you learn that red means stop ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "I know that .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Then tell me why you sped up on the yellow light .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 3
    },
    {
      "text": "I'm not sure .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "I'm giving you a ticket for this .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 3
    },
    {
      "text": "I'm sorry for the misunderstanding .",
      "speaker": "S1",
      "predicted_emotion": "sad",
      "utterance_id": 10,
      "dialog_act": 4
    },
    {
      "text": "Maybe you'll think about this before you decide to run another light .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}