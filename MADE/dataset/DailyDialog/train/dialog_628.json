{
  "id": "dialog_628",
  "segments": [
    {
      "text": "Would you like to book a table , sir ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Yes , would you arrange it for me the day after tomorrow ?",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
      "text": "Sure , when ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Six o'clock in the afternoon .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "For how many guests , please ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "Six .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "How much would you like to spend ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "We don't care about money . We'd like to have some good dishes of local specialties .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "OK .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "How many courses are there altogether , please ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "Eight .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "Could you tell me some of entrees about the dinner ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 3
    },
    {
      "text": "Sure . The main courses are roast Beijing duck and fried eel slices .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 12,
      "dialog_act": 4
    },
    {
      "text": "Very good .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 13,
      "dialog_act": 1
    }
  ]
}