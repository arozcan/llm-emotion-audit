{
  "id": "dialog_3771",
  "segments": [
    {
      "text": "Excuse me , miss . I'm Bob .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "I'm Amy . How do you do ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "I'm very glad to meet you . May I have this dance with you ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "Certainly ! I suppose you dance often .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 3,
      "dialog_act": 4
    },
    {
      "text": "No , I don't often dance . Isn't this a wonderful party ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "Yes , I'm glad I have come .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "How do you like fox-trot ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "I like it very much .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "You dance beautifully .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "Thank you . It's lucky that I have got a good partner .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Thank you .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 10,
      "dialog_act": 1
    }
  ]
}