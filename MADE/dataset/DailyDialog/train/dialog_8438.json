  "segments": [
    {
      "text": "What happened today on the freeway ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "You don't know what happened ?",
      "speaker": "S2",
