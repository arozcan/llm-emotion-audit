{
  "id": "dialog_10001",
  "segments": [
    {
      "text": "Are you going to eat anything with your sandwich ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I may get some fruit .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Oh really , what kind of fruit do you like ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "I tend to only eat grapes and apples .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "What kind of apples do you prefer ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "I've always liked green apples .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "I don't really like green apples that much .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Why do you say that ?",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "I think they are too sour .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "You'd rather eat a red apple ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "Don't you like red apples ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "They're okay , I guess .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
