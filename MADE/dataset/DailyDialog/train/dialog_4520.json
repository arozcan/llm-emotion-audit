  "segments": [
    {
      "text": "What's your favourite steps ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "It's hard to say . But I do love waltz very much .",
