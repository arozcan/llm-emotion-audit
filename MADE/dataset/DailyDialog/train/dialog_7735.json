{
  "id": "dialog_7735",
  "segments": [
    {
      "text": "Do you know why I pulled you over ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I have no idea .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Your left brake light is out .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "I was not aware of that .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "I'm going to have to write you a ticket .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "You can't give me a break ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 3
    },
    {
      "text": "I'm just doing my job .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 4
    },
    {
      "text": "How much is the ticket for ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "You will get that information in court .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "I have to go to court ?",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "That's right .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "Fine . Give me the ticket .",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 11,
      "dialog_act": 3
    }
  ]
}