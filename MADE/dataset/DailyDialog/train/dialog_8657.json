{
  "id": "dialog_8657",
  "segments": [
    {
      "text": "I heard there was an accident today .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Nobody told you what happened ?",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "Tell me the news .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "A truck turned over on the freeway .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 4
    },
    {
      "text": "Are you kidding ?",
      "speaker": "S1",
      "predicted_emotion": "surprised",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "I am completely serious .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Did anyone get hurt ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "I believe that two people did get hurt .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Were the injuries minor ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "I believe so .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "That's great to hear .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "Yes , it is .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
