  "segments": [
    {
      "text": "Taxi .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "Get on , PLS . Where do you wanna go ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "Thank you . Pls hurry , I am late . Can I get to the Battery Park before 4 ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "All right , Miss . I think we will get there if there are no delays on the way .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "How exactly do you figure out the car fare ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "According to the kilometer rate , the first five kilometers are 4 dollars and every kilometer extra costs 50 cents .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Oh , I see .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Here we are , Miss .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Thank you . How much do I owe you ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "You owe me 19 dollars .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "That's 20 dollars . Keep the change !",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 10,
      "dialog_act": 3
    },
    {
      "text": "Thank you !",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 11,
      "dialog_act": 4
    }
  ]
