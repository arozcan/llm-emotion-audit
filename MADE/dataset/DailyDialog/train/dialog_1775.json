  "segments": [
    {
      "text": "Would you like to stretch your legs ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "Why not ?",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 1,
      "dialog_act": 4
    },
    {
      "text": "Let's get a soft drink .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "Do we have enough time ?",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "Yes , we do .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "The performance is excellent .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "It's a new concert hall and the acoustics are great .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "I couldn't agree more .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Is this your first time to come to a symphony concert ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "Yes , it is .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Do you have concert halls in your city ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "Yes , but it's much smaller .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    },
    {
      "text": "Well , we'd better get back to our seats . It's about to start .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 3
    },
    {
      "text": "OK .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 13,
      "dialog_act": 4
    }
