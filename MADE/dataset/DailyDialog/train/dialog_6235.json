{
  "id": "dialog_6235",
  "segments": [
    {
      "text": "Can I help you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Yes . When is the next train to New York City ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "Let me see ... the train to New York City ... here it is ... daily except Sunday at 10 thirty , 12 twenty and 15 ten .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "Aren't there any trains before 10 thirty ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "Sorry , not before 10 thirty .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Then one to New York at 10 thirty .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 3
    },
    {
      "text": "One way or round trip ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "One way .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "A soft seat or hard one ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "How much is a soft ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "$ 15 . And for a hard one , only $ 6 .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "Then one hard seat , please .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 3
    },
    {
      "text": "OK . Here is your change . The train leaves on platform 8 .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 4
    }
  ]
}