{
  "id": "dialog_4818",
  "segments": [
    {
      "text": "Isn't it a nice day ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "It really is .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "It seems that it may rain today .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "Hopefully it will .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "How come ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "I like how clear the sky gets after it rains .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "I feel the same way . It smells so good after it rains .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "I especially love the night air when it rains .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Really ? Why ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "The stars look so much closer after it rains .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "I really want it to rain today .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "Yeah , so do I .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}