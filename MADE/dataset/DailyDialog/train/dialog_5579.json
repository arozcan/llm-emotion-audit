  "segments": [
    {
      "text": "I would like to get a bus pass .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "Do you know what kind of pass you want ?",
