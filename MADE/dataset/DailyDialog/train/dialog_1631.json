  "segments": [
    {
      "text": "Lisa , I'm so glad to see you . How are you doing ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Fine . I miss you so much , uncle Benjamin .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Me too . We haven't seen each other for years .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "It's been 3 years now .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "How time flies ! Now you are a college student out of a little girl .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "How is aunty Shirley ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "Couldn't be better .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "How has May been ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "She's been good . She is in grade one in Joy Chain high school . And she dreams to be a volunteer in 2008 .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
