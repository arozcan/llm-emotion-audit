{
  "id": "dialog_2059",
  "segments": [
    {
      "text": "My eyes water continuously . Why ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Perhaps because of the dirty air .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "What is there in the air ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Some poisonous gas .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "I can't believe that the air in this city is so dirty .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Yes . I think I won't come here again next time .",
      "speaker": "S2",
      "predicted_emotion": "disgusted",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "So do I .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "We waste a vacation .",
      "speaker": "S2",
      "predicted_emotion": "disgusted",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Maybe you are right .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    }
  ]
}