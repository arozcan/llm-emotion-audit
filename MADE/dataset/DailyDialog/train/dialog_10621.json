{
  "id": "dialog_10621",
  "segments": [
    {
      "text": "I am really thirsty .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "How about we go and get something to drink ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
      "text": "Let's do that .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 4
    },
    {
      "text": "Do you know what you want to get ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "A soda sounds good .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Soda isn't the best thing to drink when you're thirsty .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 3
    },
    {
      "text": "Why is that ?",
      "speaker": "S1",
      "predicted_emotion": "surprised",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "Soda isn't good for you .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "What should I drink then ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "You should really drink water .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 3
    },
    {
      "text": "That sounds good .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 10,
      "dialog_act": 4
    },
    {
      "text": "It's a lot better than soda .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}