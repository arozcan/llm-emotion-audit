{
  "id": "dialog_2946",
  "segments": [
    {
      "text": "This is my admission card , and these are the books .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "OK , a moment , please .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 4
    },
    {
      "text": "By the way , can I renew the borrowing ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "For which one ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "Sense and Sensibility , English version .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Yes , of course .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 4
    },
    {
      "text": "Can I just do it here ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 3
    },
    {
      "text": "Sorry . No . You should go to the other counter .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 3
    },
    {
      "text": "OK , thanks .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "Sorry , this biography is over the deadline . You should pay extra for it .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 3
    },
    {
      "text": "I was thinking of buying this from the bookstore when my father got this from the library ... anyway , how much should I pay ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "3 Yuan , please .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    },
    {
      "text": "Here you are . Thanks .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 4
    },
    {
      "text": "You are welcome . See you next time .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 13,
      "dialog_act": 1
    }
  ]
}