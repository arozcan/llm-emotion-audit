  "segments": [
    {
      "text": "What's taking you so long ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Don't worry . I'm almost finished .",
