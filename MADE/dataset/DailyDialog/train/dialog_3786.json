{
  "id": "dialog_3786",
  "segments": [
    {
      "text": "Hello , could you arrange a dinner for us ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "Sure , what time ?",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "Eight o'clock tomorrow evening .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "For how many people ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "Ten .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Western food or Chinese food ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "Chinese food .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Do you want an expensive meal ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "No , not too lavish .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "How much would you like to spend for each person ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "What is the usual thing here ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "Forty to eighty RIB Yuan a plate .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    },
    {
      "text": "Let's say sixty Yuan then .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 3
    },
    {
      "text": "OK . Is there anything special you'd like to have on the menu ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 13,
      "dialog_act": 2
    },
    {
      "text": "We'd like to have typical Chinese food .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 14,
      "dialog_act": 1
    },
    {
      "text": "Anything you'd rather not have ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 15,
      "dialog_act": 2
    },
    {
      "text": "No snakes , please .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 16,
      "dialog_act": 1
    },
    {
      "text": "How about drinks ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 17,
      "dialog_act": 2
    },
    {
      "text": "Mao Tai and Qingdao Beer .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 18,
      "dialog_act": 1
    },
    {
      "text": "Well . Dinner for ten at sixty Yuan a plate , drink's extra , at 8 o'clock tomorrow evening . Is that all right ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 19,
      "dialog_act": 2
    },
    {
      "text": "Right , thanks .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 20,
      "dialog_act": 1
    },
    {
      "text": "My pleasure , see you tomorrow evening .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 21,
      "dialog_act": 4
    },
    {
      "text": "See you then .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 22,
      "dialog_act": 1
    }
  ]
}