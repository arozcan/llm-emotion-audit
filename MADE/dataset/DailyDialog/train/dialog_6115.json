  "segments": [
    {
      "text": "Excuse me , madam . May I see your ticket , please ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "I'm afraid I have lost my ticket . Can I buy one again ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 4
    },
    {
      "text": "Where are you going ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "I'm going to Nanchang .",
