  "segments": [
    {
      "text": "Did you get your bus pass ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I haven't gone to get it yet .",
