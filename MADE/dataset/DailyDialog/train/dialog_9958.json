{
  "id": "dialog_9958",
  "segments": [
    {
      "text": "Well , what's the matter with you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I've got a sore throat and my chest hurts .",
      "speaker": "S2",
      "predicted_emotion": "sad",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Have you got a headache ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Yes , I ache all over .",
      "speaker": "S2",
      "predicted_emotion": "sad",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Are you coughing much ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "A little bit .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Now , let me listen to your chest . Ah , you have a temperature . Breathe deeply . That's right . Well , don't worry , it's only a throat infection . There's nothing wrong with your lungs . And how is your appetite ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "It's gone completely .",
      "speaker": "S2",
      "predicted_emotion": "sad",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Show me your tongue , please . All right , stay in bed and keep yourself warm . I'll prescribe some pills for you to take .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 3
    },
    {
      "text": "Is that all doctor ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "Yes . You'll be better in a few days , I'm sure .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 3
    },
    {
      "text": "Thank you , doctor . Goodbye .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 4
    },
    {
      "text": "Goodbye .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 1
    }
  ]
}