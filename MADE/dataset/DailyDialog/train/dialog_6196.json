    {
      "text": "What is your salary now ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "My present pay is RIB 3 , 000 yuan each month .",
      "speaker": "S2",
