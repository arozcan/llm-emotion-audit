{
  "id": "dialog_1420",
  "segments": [
    {
      "text": "Why don't you come round for a meal one evening next week ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "I'd love to .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 1,
      "dialog_act": 4
    },
    {
      "text": "Which day would suit you ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Any day except Tuesday .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "How about Thursday ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "Yes , Thursday would be fine . What time shall I come ?",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 5,
      "dialog_act": 3
    },
    {
      "text": "Oh , about 6:00 . Will that be OK ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 6,
      "dialog_act": 3
    },
    {
      "text": "Yes , of course . Thank you very much .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 7,
      "dialog_act": 4
    },
    {
      "text": "See you on Thursday , then .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 8,
      "dialog_act": 1
    }
  ]
}