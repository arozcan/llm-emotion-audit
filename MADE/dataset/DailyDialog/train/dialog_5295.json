{
  "id": "dialog_5295",
  "segments": [
    {
      "text": "Do you have any idea why I pulled you over today ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I'm not sure .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "One of your brake lights is out .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "Really ? I'm sorry . I did not know that .",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "I'm going to give you a ticket .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "Why can't you just let me off with a warning ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 3
    },
    {
      "text": "It's just a ticket .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 4
    },
    {
      "text": "How much is the fine ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "The judge will let you know .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "I have to be at court ?",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "Of course .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "All right . Just give me the ticket .",
      "speaker": "S2",
      "predicted_emotion": "sad",
      "utterance_id": 11,
      "dialog_act": 3
    }
  ]
}