  "segments": [
    {
      "text": "Good evening , Yanking Restaurant .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Could I reserve a table for Saturday , September 16th at 8: 00 p . m . ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
