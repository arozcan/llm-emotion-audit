{
  "id": "dialog_967",
  "segments": [
    {
      "text": "I'm here for my appointment .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "What can I do for you today ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "I have an issue with my schedule .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "Tell me the problem .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 3
    },
    {
      "text": "I have two classes on here that are at the same time .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Oh , I see .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Can you change that for me ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 3
    },
    {
      "text": "Do you still want both classes ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "Yes , I would like to keep both classes .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 3
    },
    {
      "text": "I'll try to change one to a different day .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 4
    },
    {
      "text": "I would greatly appreciate that .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 10,
      "dialog_act": 1
    }
  ]
