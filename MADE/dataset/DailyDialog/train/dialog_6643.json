{
  "id": "dialog_6643",
  "segments": [
    {
      "text": "Good afternoon ! Where to ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Tian'anmen Square .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "You are going to Tian'anmen Square . No problem .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "How long will it take ? We are in a great hurry .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "Around 20 minutes , but it all depends on the traffic .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Hope there is no traffic jam .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "By this time , it's better . The rush hour is over .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Could you drop us off on that corner ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 3
    },
    {
      "text": "Sure thing . Here we are .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "How much do we owe you ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "Eight yuan twenty cents .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "Here is ten yuan . Keep the change , please .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 3
    },
    {
      "text": "Thank you . Have a nice day .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 12,
      "dialog_act": 4
    },
    {
      "text": "Thanks and you too .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 13,
      "dialog_act": 1
    }
  ]
}