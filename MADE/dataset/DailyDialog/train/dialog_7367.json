{
  "id": "dialog_7367",
  "segments": [
    {
      "text": "Taxi !",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "Where would you like to go ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "Please rush me to the railway station",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "Get in and close the door .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 4
    },
    {
      "text": "Can you speed up a bit ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "Sorry . The road is too crowded .",
      "speaker": "S2",
      "predicted_emotion": "sad",
      "utterance_id": 5,
      "dialog_act": 4
    },
    {
      "text": "But I have no time to lose .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "But I can't help it .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Can you drop me off here ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 3
    },
    {
      "text": "Sorry . We mustn't stop here .",
      "speaker": "S2",
      "predicted_emotion": "sad",
      "utterance_id": 9,
      "dialog_act": 4
    }
  ]
}