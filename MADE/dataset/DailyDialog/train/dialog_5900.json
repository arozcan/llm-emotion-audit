{
  "id": "dialog_5900",
  "segments": [
    {
      "text": "Plaza Hotel , good morning . Leo speaking .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Ah yes , I'd like to book two rooms for myself and my father . Could you tell me the cost of a single room per night ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
      "text": "Certainly . A single room is 120 dollars American , per night .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 4
    },
    {
      "text": "Fine .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "And when would you like the rooms ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "From the 25th to the 28tri of September .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Arriving the 25th of September and leaving on the 28th ? Three nights ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "That's right .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Just a minute please .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "Yes , we have rooms available then . You require two single rooms ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "Yes , thank you .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "Can l have your name please ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 3
    },
    {
      "text": "My name is Mona White .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 4
    },
    {
      "text": "And your father's name , Ms White ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 13,
      "dialog_act": 3
    },
    {
      "text": "Jack Webber .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 14,
      "dialog_act": 4
    },
    {
      "text": "Could you spell the surname please ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 15,
      "dialog_act": 3
    },
    {
      "text": "Sure . W-E-double B-E-R .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 16,
      "dialog_act": 4
    },
    {
      "text": "Double P for Papa ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 17,
      "dialog_act": 2
    },
    {
      "text": "No , double B for Bravo",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 18,
      "dialog_act": 1
    }
  ]
}