  "segments": [
    {
      "text": "Hey , how's it going ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Good . How about you ?",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "Not bad , thanks for asking .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "Have you been on the bus for a while ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "Only about fifteen minutes .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Do you ride this bus often ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "Not really , I usually drive .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "You have a car ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "Yes , I do .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "So why aren't you driving it ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "Once our President lowers gas prices , I'll be driving again .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "That's smart thinking .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 11,
      "dialog_act": 1
    }
