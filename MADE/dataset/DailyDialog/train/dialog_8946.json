{
  "id": "dialog_8946",
  "segments": [
    {
      "text": "Why do you want to take the police to court ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "They arrested me without probable cause .",
      "speaker": "S2",
      "predicted_emotion": "angry",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "They did ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "They had no reason for arresting me .",
      "speaker": "S2",
      "predicted_emotion": "angry",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "I thought they let you go .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Yes , the next morning .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "They made you spend the night at the station ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "Yes , they did .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "What was their reason for arresting you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "I matched the description of a bank robber they were looking for .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "I see .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "I want justice .",
      "speaker": "S2",
      "predicted_emotion": "angry",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}