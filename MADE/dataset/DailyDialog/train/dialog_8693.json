{
  "id": "dialog_8693",
  "segments": [
    {
      "text": "What's for dinner tonight ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "It's whatever you're planning on cooking .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "You're not going to cook ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "No , I always cook .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Yeah , and I love your cooking .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Don't give me that . If you're hungry , then cook dinner .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 3
    },
    {
      "text": "You're being serious ?",
      "speaker": "S1",
      "predicted_emotion": "surprised",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "Look , I'm really tired tonight . I don't feel like cooking dinner .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Okay , I'll make dinner tonight .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "Thank you . I'm going to bed .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Do you want me to wake you up when dinner is ready ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "Yes , please .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}