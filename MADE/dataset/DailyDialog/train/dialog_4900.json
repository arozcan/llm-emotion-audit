      "dialog_act": 1
    },
    {
      "text": "What's wrong ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Someone robbed my apartment last week . The robber took $ 550 .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Oh , no ! Did you call the police ?",
      "speaker": "S1",
