{
  "id": "dialog_5500",
  "segments": [
    {
      "text": "Can I help you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I want to buy a hat .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Come here , please .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "Is this one fit for you ? It is nice .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 3
    },
    {
      "text": "I don't like the style .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 4
    },
    {
      "text": "How about this one ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 3
    },
    {
      "text": "Black is not my favorite colour .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 4
    },
    {
      "text": "You must like this pink one , don't you ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "It's pretty . How much is it ?",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "Fifty yuan .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "It's too expensive . Can you come down a little ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "How about forty-five ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 3
    },
    {
      "text": "I'll buy it if forty .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 4
    },
    {
      "text": "OK .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 13,
      "dialog_act": 1
    }
  ]
