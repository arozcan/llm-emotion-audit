{
  "id": "dialog_8879",
  "segments": [
    {
      "text": "Why are you arresting me ?",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "There is a warrant out for your arrest .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "What kind of warrant ?",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "You have a bench warrant .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "What is that ?",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "Have you missed a court date ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "I never went to court .",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "I can't help you .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "This is unfair .",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "A lot of things aren't fair . That's life .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "I have rights , don't I ?",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "You have the right to an attorney .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}