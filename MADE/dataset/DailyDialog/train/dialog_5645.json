  "segments": [
    {
      "text": "Good afternoon , Mr . Anderson .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Good afternoon , Miss Li .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "How nice to see you again !",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "Nice to see you , too .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "It's been a long time , hasn't it ? How are you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "Fine , thank you . And you ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "Very well too , is there anything I can do for you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "Yes . Could you guide me to where I can change money ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 3
    },
    {
      "text": "Sure , Mr . Anderson , please come along with me .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "Thank you .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "My pleasure .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    }
  ]
