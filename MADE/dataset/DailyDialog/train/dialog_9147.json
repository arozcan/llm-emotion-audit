  "segments": [
    {
      "text": "Hi Michael .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Hi Amy . What's up ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "I'm looking for the airport . Can you tell me how to get there ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "No , sorry . I don't know .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 4
    },
    {
      "text": "I think I can take the subway to the airport . Do you know where the subway is ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "Sure , it's over there .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Where ? I don't see it .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "Across the street .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Oh , I see it now . Thanks .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "No problem .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Do you know if there's a restroom around here ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "Yes , there's one here . It's in the store .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    },
    {
      "text": "Thank you .",
      "speaker": "S1",
      "predicted_emotion": "happy",
