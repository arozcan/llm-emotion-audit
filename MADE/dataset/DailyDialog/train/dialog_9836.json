{
  "id": "dialog_9836",
  "segments": [
    {
      "text": "What are you here for today ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I need to fight this ticket that I got .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
      "text": "Is the arresting officer here ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Yes , he is , Your Honor .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Plead your case .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "I was told by the officer that I ran a red light . That information isn't true .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Why would the officer lie ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "I'm not sure , but the signal had a camera on it .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Did the camera take a picture of your license plate ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "No , my picture wasn't taken .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "There was no picture taken , so I'll let you go .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 3
    },
    {
      "text": "Thank you very much , Your Honor .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 11,
      "dialog_act": 4
    }
  ]
}