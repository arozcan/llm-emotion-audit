{
  "id": "dialog_3475",
  "segments": [
    {
      "text": "Can you play tennis ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Yes , I can .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "It is interesting , isn't it ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Yes , it's very interesting .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Can you teach me ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "Sure . Take the tennis racket . Now , throw the ball up , and hit it with the racket . Like this .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 4
    },
    {
      "text": "Let me try . Oh , I missed !",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Throw the ball up high .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "High ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "Yes , very high . Over your head . Then you will have time to hit the ball .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Oh ! I hit it .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "Well done !",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 11,
      "dialog_act": 1
    },
    {
      "text": "Thank you . I think I like playing it .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 12,
      "dialog_act": 1
    }
  ]
}