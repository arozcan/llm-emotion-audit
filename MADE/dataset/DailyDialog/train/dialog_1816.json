  "segments": [
    {
      "text": "I heard you are going to move .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Yes , I found a better house near my office . I ’ m very satisfied with it . But I ’ m worrying .",
