{
  "id": "dialog_5327",
  "segments": [
    {
      "text": "What can I do for you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I wish to buy a diamond ring .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "How many carats diamond do you want ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Two carats .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Is this one suit for you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "No , it seems too old-fashioned .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 4
    },
    {
      "text": "What about this ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 3
    },
    {
      "text": "Let me try it on , it's too small for me , haven't you got any larger ones ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 4
    },
    {
      "text": "Yes , try this one please .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "This fits me well , how much is it ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "3,500 yuan .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "It's reasonable . Here you are .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
