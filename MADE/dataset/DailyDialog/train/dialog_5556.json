  "segments": [
    {
      "text": "How much is the postage for this postcard to China ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "How would you like to send it ?",
      "speaker": "S2",
