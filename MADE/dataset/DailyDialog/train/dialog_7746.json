{
  "id": "dialog_7746",
  "segments": [
    {
      "text": "Are you Miss Thou Ping ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Yes . I am Thou Ping .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "How do you spell you first name ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Ping . P-I-N-G .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Do you have an English name ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "Yes , sir . It is Jane . It was given by my English professor when I was at the university .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "What ’ s your pen name ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "My pen name is ' Dark Blue ' .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "How do you like your name ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "I like it very much . My name has became part of me . I don ’ t want to change it .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "What ’ s your present ( permanent ) address ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "My present address is No . 21 South Zhongshan Road , Apt . 19 , Shanghai . The post code is 220150 .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    },
    {
      "text": "You look very young . How old are you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 2
    },
    {
      "text": "Actually , I ’ m thirty now .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 13,
      "dialog_act": 1
    },
    {
      "text": "Where were you born ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 14,
      "dialog_act": 2
    },
    {
      "text": "I was born in Xinjiang .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 15,
      "dialog_act": 1
    },
    {
      "text": "What is your nationality ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 16,
      "dialog_act": 2
    },
