{
  "id": "dialog_11035",
  "segments": [
    {
      "text": "Good afternoon . May I help you ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Yes . I'd like to check in , please .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
      "text": "May I see your ticket and passport , please ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "Sure . Here they are .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 4
    },
    {
      "text": "Please put your baggage on the scale .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "What's the weight limit ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "Thirty two kilos .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "I hope my baggage aren't overweight .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "No . They aren't .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "May I carry this satchel ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 3
    },
    {
      "text": "Sure . But please attach this claim tag .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 3
    },
    {
      "text": "OK .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 4
    },
    {
      "text": "Here is your boarding pass .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 1
    },
    {
      "text": "Which gate ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 13,
      "dialog_act": 2
    },
    {
      "text": "Gate Four . Please go and have a seat in the waiting hall .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 14,
      "dialog_act": 3
    },
    {
      "text": "Thank you .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 15,
      "dialog_act": 4
    }
  ]
}