{
  "id": "dialog_7544",
  "segments": [
    {
      "text": "Is there a bus in Altadena that'll take me to downtown LA ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "The 486 .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "The 486 really goes downtown ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Yes , it does .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "How long is the bus ride ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "It's about forty-five minutes to an hour .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Is that right ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "Yeah , it's not that long of a trip .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Is that a crowded bus ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "It starts getting more crowded the closer you get to LA .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Thank you very much .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "You're very welcome .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}