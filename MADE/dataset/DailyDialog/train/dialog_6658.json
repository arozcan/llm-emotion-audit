{
  "id": "dialog_6658",
  "segments": [
    {
      "text": "I don't understand why I'm being arrested .",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "You have a warrant .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "A warrant for what ?",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "Apparently , you have a bench warrant .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "What does that mean ?",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "You missed a court appearance .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "I wasn't aware that I had to go to court .",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "I don't know what to tell you .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "I don't find this fair .",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "Life isn't fair .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Don't I have any rights ?",
      "speaker": "S1",
      "predicted_emotion": "angry",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "You have the right to remain silent .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}