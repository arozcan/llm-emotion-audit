  "segments": [
    {
      "text": "Well , here is your breakfast !",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "Thanks so much . Miss , I believe I ordered my eggs scrambled , and these are fried .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 4
    },
    {
      "text": "Sorry , your friend over there ordered fried eggs , and I gave you his by mistake .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "Oh yeah . Here , I will just trade with him .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Here are your pancakes , madam .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "But I ordered waffles !",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Oh , I am so sorry !",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "That's OK . I will eat my eggs and bacon , and you can take my pancakes back .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 3
    },
    {
      "text": "Good , sir . I will take care of that right away .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "Thank you so much !",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    }
  ]
