{
  "id": "dialog_6740",
  "segments": [
    {
      "text": "I want to send some money to Nanchang . Is it handled here ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Yes , sir . How much would you like to remit ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
      "text": "I want to remit 1,200 yuan to my brother in Nanchang .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 3
    },
    {
      "text": "No problem . Do you want the money to go by M / T or T / T ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "What's the difference ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "If you take M / T , it will take a longer time for your brother to receive the money .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "OK . I'll take T / T .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 3
    },
    {
      "text": "Would you please fill in this application form ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 3
    },
    {
      "text": "All right . Here it is .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 4
    },
    {
      "text": "For 1,200 yuan to Nanchang , the commission is 12 yuan .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 3
    },
    {
      "text": "Here you are . Thanks .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 4
    },
    {
      "text": "You are welcome .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    },
    {
      "text": "Bye-bye .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 1
    }
  ]
}