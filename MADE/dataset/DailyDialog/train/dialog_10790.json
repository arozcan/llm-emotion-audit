{
  "id": "dialog_10790",
  "segments": [
    {
      "text": "Excuse me , I'm looking for the airport bus stop . Do you know where it is ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Here it is .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "When does the bus come ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "You just missed one .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "How about the next one ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "In an hour .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Isn't there an earlier one ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "No , they come every hour on the hour .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Thank you very much .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "You are welcome .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 9,
      "dialog_act": 1
    }
  ]
