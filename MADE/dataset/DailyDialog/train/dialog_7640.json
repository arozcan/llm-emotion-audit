  "segments": [
    {
      "text": "Please pull your vehicle over to the side of the road . Please roll down your window .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 3
    },
    {
      "text": "What's the matter , sir ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 2
    },
    {
      "text": "Have you been drinking ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "No , I haven't , sir .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Really ? But I can smell alcohol on your breath . Blow to the breathalyzer , please .",
      "speaker": "S1",
      "predicted_emotion": "surprised",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "But sir , I didn't drink at all .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 4
    },
    {
      "text": "If you refuse the blow , you'll receive a ticket and your vehicle will be impounded .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 3
    },
    {
      "text": "OK , I will blow .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 4
    },
    {
      "text": "Your BAC exceeds legal limit .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "But I ...",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "I'll have to write you a ticket and impound your vehicle .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 3
    },
    {
      "text": "Oh , no !",
      "speaker": "S2",
      "predicted_emotion": "angry",
      "utterance_id": 11,
      "dialog_act": 4
    }
  ]
