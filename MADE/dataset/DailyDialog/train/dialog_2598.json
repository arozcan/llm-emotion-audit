{
  "id": "dialog_2598",
  "segments": [
    {
      "text": "Has Alan shown up yet ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Nope . I guess something might have come up .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "I wonder what happened .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "I don't know . I hope it's nothing serious .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Last week his sister was admitted to the hospital .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Oh ? What happened ?",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "His sister had a car accident last week .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Is she fine now ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "She is still in a coma .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "Oh , poor Alan ! His sister is the only one he has left since his parents died last year .",
      "speaker": "S2",
      "predicted_emotion": "sad",
      "utterance_id": 9,
      "dialog_act": 1
    }
  ]
}