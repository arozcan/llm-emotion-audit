{
  "id": "dialog_10644",
  "segments": [
    {
      "text": "Is there a problem , officer ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I stopped you for driving through a red light .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Did I really ?",
      "speaker": "S1",
      "predicted_emotion": "surprised",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "You didn't know ?",
      "speaker": "S2",
      "predicted_emotion": "surprised",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "I had no idea that I ran it .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Don't you know that yellow means slow down ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "Yes , madam .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "Why'd you speed up instead ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "I really have no excuse , madam .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "I'll have to write you a ticket .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 3
    },
    {
      "text": "I understand , and I am very sorry .",
      "speaker": "S1",
      "predicted_emotion": "sad",
      "utterance_id": 10,
      "dialog_act": 4
    },
    {
      "text": "Here's your ticket .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}