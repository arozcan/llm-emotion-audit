{
  "id": "dialog_4564",
  "segments": [
    {
      "text": "Hello , is this People's Theater ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "Yes , it is . May I help you ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 3
    },
    {
      "text": "Yes , are advance tickets for ' Nut croakers ' still available ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "What date , sir ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 2
    },
    {
      "text": "October 16th .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "Let me see . Yes , we still have some tickets left . Which performance ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 2
    },
    {
      "text": "7:30 p . m . performance , please .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "OK , we have tickets at 100 yuan and 50 yuan .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "I'd like to reserve two seats at 100 yuan , please .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 3
    },
    {
      "text": "All right . May I have you name , please ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 2
    },
    {
      "text": "Du Kun .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "When would you like to collect the tickets ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 2
    },
    {
      "text": "Tomorrow .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 12,
      "dialog_act": 1
    },
    {
      "text": "We can hold them until one day before the show .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 13,
      "dialog_act": 4
    },
    {
      "text": "How do we cancel , just in case ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 14,
      "dialog_act": 2
    },
    {
      "text": "Just telephone us , and let us know in advance . We don't charge for cancellation .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 15,
      "dialog_act": 1
    },
    {
      "text": "All right , thank you very much . Good bye .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 16,
      "dialog_act": 1
    },
    {
      "text": "You are welcome . Goodbye .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 17,
      "dialog_act": 1
    }
  ]
