{
  "id": "dialog_6617",
  "segments": [
    {
      "text": "Have you heard about the robbery ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 2
    },
    {
      "text": "I saw the whole thing happen .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "For real ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "I was in the bank during the robbery .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "What went down ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 2
    },
    {
      "text": "Some dude came into the bank with a gun .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Are you serious ?",
      "speaker": "S1",
      "predicted_emotion": "surprised",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "When he pulled out the gun , I got so scared .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Tell me what else happened .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 3
    },
    {
      "text": "He got the money and tried to leave .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Did the cops get him ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "The cops arrested him before he could even make it out the door .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
}