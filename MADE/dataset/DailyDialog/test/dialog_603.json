{
  "id": "dialog_603",
  "segments": [
    {
      "text": "I can't believe how hot it is .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "It's not even noon yet .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "That means it will get hotter .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "I am dying from the heat .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "Turn on the air conditioner .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 4,
      "dialog_act": 3
    },
    {
      "text": "It doesn't work .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 5,
      "dialog_act": 4
    },
    {
      "text": "What happened ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 6,
      "dialog_act": 2
    },
    {
      "text": "I don't know .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Did you call the repairman ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 8,
      "dialog_act": 2
    },
    {
      "text": "Of course .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "When is he coming ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 2
    },
    {
      "text": "He's busy . He said next week .",
