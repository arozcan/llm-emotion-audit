  "segments": [
    {
      "text": "I hate to go outside .",
      "speaker": "S1",
      "predicted_emotion": "disgusted",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Me too .",
      "speaker": "S2",
      "predicted_emotion": "disgusted",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Why do you hate to go outside ?",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 2
    },
    {
      "text": "I meet too many jerks .",
      "speaker": "S2",
      "predicted_emotion": "disgusted",
      "utterance_id": 3,
      "dialog_act": 1
    },
    {
      "text": "I agree .",
      "speaker": "S1",
      "predicted_emotion": "disgusted",
      "utterance_id": 4,
      "dialog_act": 1
    },
    {
      "text": "This city is full of jerks .",
      "speaker": "S2",
      "predicted_emotion": "disgusted",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Rude people are everywhere .",
      "speaker": "S1",
      "predicted_emotion": "disgusted",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "But what can you do ?",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 7,
      "dialog_act": 2
    },
    {
      "text": "You can yell at them .",
      "speaker": "S1",
      "predicted_emotion": "disgusted",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "And they will yell back at you .",
      "speaker": "S2",
      "predicted_emotion": "disgusted",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "Yelling doesn't do any good .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 10,
      "dialog_act": 1
    },
    {
      "text": "No . The best thing to do is just stay home .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 11,
      "dialog_act": 1
    }
  ]
