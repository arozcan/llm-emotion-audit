{
  "id": "dialog_980",
  "segments": [
    {
      "text": "It's quiet everywhere in winter .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 0,
      "dialog_act": 1
    },
    {
      "text": "Yes . I like winter .",
      "speaker": "S2",
      "predicted_emotion": "neutral",
      "utterance_id": 1,
      "dialog_act": 1
    },
    {
      "text": "Me too .",
      "speaker": "S1",
      "predicted_emotion": "neutral",
      "utterance_id": 2,
      "dialog_act": 1
    },
    {
      "text": "It's snowing heavily . What about taking a walk ?",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 3,
      "dialog_act": 3
    },
    {
      "text": "That's a good idea . Let's go !",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 4,
      "dialog_act": 4
    },
    {
      "text": "What a heavy snow ! Look ! The water is frozen .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 5,
      "dialog_act": 1
    },
    {
      "text": "Take care ! Don't slip on the ground .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 6,
      "dialog_act": 1
    },
    {
      "text": "I've got it . I like the feeling of stepping on the ice .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 7,
      "dialog_act": 1
    },
    {
      "text": "Yes . Very wonderful .",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 8,
      "dialog_act": 1
    },
    {
      "text": "There is a snowman over there .",
      "speaker": "S2",
      "predicted_emotion": "happy",
      "utterance_id": 9,
      "dialog_act": 1
    },
    {
      "text": "How lovely it is !",
      "speaker": "S1",
      "predicted_emotion": "happy",
      "utterance_id": 10,
      "dialog_act": 1
    }
  ]
}